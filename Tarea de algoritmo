	Escribir "Cuantos años has estado trabajando con nosotros"
	leer años
	Escribir "Cual es tu salario"
	leer salario
	Repetir
		
	Hasta Que (años > 5)
	Si años >= 5 Entonces
		calculo = salario * 5 / 100
		bono = salario + calculo
	SiNo
		Escribir "No aplicas a ningun bono"
	Fin Si
	Escribir "Usted tiene un bono del 5% por estar con nosotros mas de 5 años"
	Escribir "Su nuevo salario gracias al bono es de:" , bono "$"
	
