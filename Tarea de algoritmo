Funcion ejercicio_video2
	//Definimos las variable num1, num2, total como enteras
	//Pedimos al usuario que ingrese el primer numero
	//Leer num1
	//Pedimos al usuario que ingrese el segundo numero
	//Leer num2
	//total = num1 + num2
	//Mostramos el resultado
	Escribir "Suma"
	Definir num1, num2, total como entero
	Escribir "Ingrese un numero"
	leer num1
	Escribir "Ingrese el segundo numero"
	leer num2
	total = num1 + num2 
	Escribir "La suma de los dos numeros es de:" total
FinFuncion

Funcion ejercicio_video3
	//Definimos la variable edad como entera
	//Pedimos al usuario que ingrese su edad
	//Leemos edad
	//Usamos el comando Si-Entonces
	//Si edad > 18 Entonces escribir eres mayor de edad, SiNo escribimos eres menor de edad
	Escribir "Definir edad"
	Definir edad Como Entero
	Escribir "Ingrese su edad"
	leer edad
	Si edad >= 18 Entonces
		Escribir "Eres mayor de edad"
	SiNo
		Escribir "Eres menor de edad"
	Fin Si
	
FinFuncion

Funcion ejercicio_video4
	//Las variables se asignan
	//sed = "si"
	//dinero = "si"
	//Usamos el comando Si-Entonces
	//Si sed = "si" y dinero = "si" entonces escribimos "compra una botella de agua"
	//SiNo; Si sed = "no" y dinero = "si" entonces escribimos "compra un chocolate"
	//Usamos otro comando Si-Entonces
	//Si sed = "no" y dinero = "si" entonces escribimos "compra un chocolate"
	//SiNo; escribimos "no tienes dinero, ve a casa"
	Escribir "Operador logico"
	sed = "si"
	dinero = "si"
	Si sed = "si" y dinero = "si" Entonces
		Escribir "Compra una botella de agua"
	SiNo
		Si sed = "no" y dinero = "si" Entonces
			Escribir "Compra un chocolate"
		SiNo
			Escribir "No tienes dinero, ve a casa...."
		FinSi
	Fin Si
	
FinFuncion

Funcion ejercicio_video5
	//Definimos las variables numaleatorio, num1, intentos como entero
	//numaleatorio = azar(10)
	//Pedimos al usuario que ingrese un numero
	//Leer num1
	//intentos = 5
	//Usamos un ciclo Mientras 
	//Usamos el comando Si-Entonces dentro del ciclo Mientras 
	Escribir "Ciclo Mientras"
	Definir numaleatorio, num1, intentos Como Entero
	numaleatorio = azar(10)
	Escribir "Ingrese un numero"
	leer num1
	intentos = 5
	Mientras intentos > 0 Hacer
		Si numaleatorio = num1 Entonces
			Escribir "Has acertado, el numero secreto era:" , numaleatorio
			intentos = 0
		SiNo
			intentos = intentos - 1
			Escribir "Fallaste, te quedan " , intentos " intentos"
		FinSi
	Fin Mientras
	Si  intentos = 0 Entonces
		Escribir "Ya no te quedan mas intentos; Perdiste"
	SiNo
		Escribir "Ganaste"
	Fin Si
	
FinFuncion

Funcion ejercicio_video6
	//Definimos combo como una variable entera
	//Escribimos "Que combo desea"
	//Escribimos "1 : combo 1
	//Escribimos "2 : combo 2
	//Escribimos "2 : combo 3
	//Leer combo
	//Usamos el condicional Segun para realizar el ejercicio
	Escribir "Condicional Segun"
	Definir combo Como Entero
	Escribir "Que combo desea"
	Escribir "1 : combo 1"
	Escribir "2: combo 2"
	Escribir "3 : combo 3"
	leer combo
	Segun combo Hacer
		1:
			Escribir "El valor es de $5000"
		2:
			Escribir "El valor es de $2500"
		3:
			Escribir "El valor es de 1000"
		De Otro Modo:
			Escribir "No tenemos lo que buscas"
	Fin Segun
	
FinFuncion

Funcion ejercicio_video7
	//Definimos la variable num como entero
	//Definimos la variable respuesta como caracter
	//Usamos el ciclo Repetir
	//num = azar(10); escribimos  "el numero aleatorio es:" num1
	//Escribimos "Deseas otro numero?"
	//Leer respuesta
	//Hasta Que respuesta = "no"
	Escribir "Ciclo Repetir"
	Definir num Como Entero
	Definir respuesta Como Caracter
	Repetir
		num = azar(10)
		Escribir "El numero aleatorio es:", num
		Escribir "Deseas otro numero?"
		leer respuesta
	Hasta Que respuesta = "n"
	
FinFuncion

Funcion ejercicio_video8
	//Usamos Dimension personas(3)
	//personas(1) = "Ignacio"
	//personas(2) = "Victor"
	//personas(3) = "Juanito"
	//Usamos el comando "Para"; donde i = 1 hasta 3 con paso 1 Hacer Escribir "El nombre de mi arreglo es: ", personas(i)
	Escribir "Arreglos y For"
	Dimension personas(3)
	personas(1) = "	Ignacio"
	personas(2) = "Victor"
	personas(3) = "Juanito"
	Para i = 1 Hasta 3 Con Paso 1 Hacer
		Escribir "El nombre del arreglo es:", personas(i)
	Fin Para
	
FinFuncion

