	//Pedimos al usuario que ingrese el valor de los grados celsius
	//Leer cel
	//fare = (cel * (9/5)) + 32 
	//Mostramos el resultado
	Escribir "Funcion con return para convertir grados celsius a farenheit"
	Escribir "Igrese el valor de los grados celsius"
	leer cel
	fare = (cel * (9/5)) + 32
	Escribir "El resultado es: " , fare
	
FinFuncion

Funcion ejercicio65
	//Ejercicio N°65 "Funcion para contar un caracter en una frase"
	//Definimos las variables a, b como enteras
	//Definimos las variables frase, c como caracter
	//a = Longitud(frase) 
	//Pedimos al usuario que ingrese una frase
	//leemos frase
	//Usamos ciclo "Para"
	Escribir "Funcion para contar un caracter en una frase"
	Definir a, b Como Entero
	Definir frase, c Como Caracter
	a = Longitud(frase)
	Escribir "Ingrese una frase"
	leer frase
	Para i = 0 Hasta a Con Paso 1 Hacer
		c = Subcadena(frase,i,i) //Evaluamos letra por letra
		Si c <> "" Entonces
			frase = frase + c 
			
		Fin Si
	Fin Para
	a = Longitud(frase)
	Escribir "La cantidad de caracteres en la frase es" , a
FinFuncion

Funcion ejercicio66
	//Ejercicio N°66 "Funcion sin Return para mostrar numeros del 1 al 10"
	//Definimos num como variable entera
	//Usamos el bucle "Para"
	Escribir "Funcion sin Return para mostrar numeros del 1 al 10"
	Definir num Como Entero
	Escribir "Numeros del 1 al 10"
	Para num = 1 Hasta 10 Con Paso 1 Hacer
		num = num + 1
	Fin Para
	Escribir num
FinFuncion

Funcion ejercicio67
	//Ejercicio N°67 "Funcion con parametros y return para sumar una lista de numeros"
	//Definimos num1, suma como real
	//suma = 0
	//Pedimos al usuario que ingrese la lista de numeros
	//Usamos el ciclo Mientras que ira sumando los numeros 
	Escribir "Funcion con parametros y return para sumar una lista de numeros"
	Definir num1, suma Como Real
	suma = 0
	Mientras c > 0 Hacer
		Escribir "Ingrese un numero"
		leer num1 
		suma = suma + num1 
		c = c - 1
	Fin Mientras
FinFuncion
