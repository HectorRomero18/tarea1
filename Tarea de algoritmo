	Funcion ejercicio11
	//Ejercicio N°11 "Suma de dos numeros"
	//Definimos a,b,c como variables enteras
	//Pedir al usuario que ingrese el primer valor
	//Leer variable a
	//Pedir al usuario que ingrese el segundo valor
	//Leer variable b
	//La variable c tomara los valores de las variables a y b sumados	
		//Escribimos "La suma de los dos valores es:", c
		Escribir "Suma de dos numeros"
		Definir a,b,c Como Entero
		Escribir "Ingrese el primer valor"
		leer a
		Escribir "Ingrese el segundo valor"
		leer b
		c = a + b
		Escribir "La suma de estos dos valores es:", c
	
FinFuncion

Funcion ejercicio12
	//Ejercicio N°12 "Calcular el area de un triangulo"
	//Definimos a,c,z como variables reales
	//Pedir al usuario que ingrese el valor de la base del triangulo
	//Leer variable a
	//Pedir que el usuario ingrese el valor de la altura del triangulo
	//Leer la variable b
	//Las variables a y b se multiplicaran y luego se dividiran para 2
	//La variable z tomara el valor del resultado anterior
	//Escribimos "El area ddel triangulo es:", z
	Escribir "Calcular el area de un triangulo"
	Definir a,b,z Como real
	Escribir "Ingrese el valor de la base del triangulo"
	leer a
	Escribir "Ingrese el valor de la altura del triangulo"
	leer b
	z = a * b / 2
	Escribir "El area del triangulo es:" , z
	
FinFuncion

Funcion ejercicio13
	//Ejercicio N°13 "Determinar si un numero es par o impar
	//Definimos a,b como variables reales
	//Pedir al usuario que ingrese un numero
	//La variable a tomara el numero ingresado por el usuario
	//La variable b = a mod 2
	//Usamos el comando Si-Entonces para determinar si es un numero par o impar
	Escribir "Determinar si un numero es par o impar"
	Definir a,b Como real
	Escribir "Ingrese un numero"
	leer a
	b = a mod 2
	Si b = 0 Entonces
		Escribir "Es un numero par"
	SiNo
		Escribir "Es un numero impar"
	Fin Si
FinFuncion

Funcion ejercicio14
	//Ejercicio N°14 "Calculadora simple"
	//Definimos n1,n2,opc,resultado como variables enteras
	//Pedir al usuario que ingrese un numero
	//Leemos la variable n1
	//Pedir al usuario que ingrese el segundo numero
	//Leer n2
	//Escribimos "Ingrese una opcion" donde 1.= sumar; 2.=restar; 3.=multiplicar; 4.=dividir
	//Usamos el comando Segun para elaborar la operacion
	Escribir "Calculadora simple"
	Definir n1, n2, opc, resultado como entero
	Escribir "Ingrese un numero"
	leer n1
	Escribir "Ingrese el segundo numero"
	leer n2
	Escribir "Ingrese una opcion"
	Escribir "1. Sumar" 
	Escribir "2. Restar"
	Escribir "3. Multiplicar"
	Escribir "4. Dividir"
	leer opc
	Segun opc Hacer
		1:
			resultado = n1 + n2;
			Escribir "La suma es:", resultado; 
		2:
			resultado = n1 - n2;
			Escribir "La resta es:", resultado
		3:
			resultado = n1 * n2
			Escribir "La multiplicacion es:", resultado
		4:
			resultado = n1 / n2
			Escribir "La division es:", resultado
		De Otro Modo:
			Escribir "Opcion incorrecta"
	Fin Segun
FinFuncion

Funcion ejercicio15
	//Ejercicio N°15 "Tabla de multiplicar"
	//Definimos las variables num1, t, i, resultado como enteras
	//Pedir al usuario que ingrese un numero
	//La variable t = 10
	//Usamos el comando "Para" para ejecutar la operaion
	Escribir "Tabla de multiplicar"
	Definir num1, t, i, resultado Como Entero
	Escribir "Ingrese un numero"
	leer num1
	t = 10
	Para i = 0 Hasta t Con Paso 1 Hacer
		resultado = num1 * i
		Escribir num1  "X"  i, " = ", resultado;
	Fin Para
FinFuncion

Funcion ejercicio16
	//Ejercicio N°16 "Concatenar palabras"
	//Definimos cadena1, cadena2, cadena3 como variables caracter
	//Pedimos que ingrese una palabra
	//La variable cadena1 tomara la palabra antes escrita
	//Pedimos que ingrese otra palabra
	//La variable cadena2 tomara la segunda palabra
	//Usamos la funcion concatenar para unir las dos palabras
	//La variable cadena3 guardara las palabras concatenadas
	//Escribimos la variable cadena3
	Escribir "Concatenar palabras"
	Definir cadena1, cadena2, cadena3 Como Caracter
	Escribir "Ingrese una palabra"
	leer cadena1
	Escribir "Ingrese otra palabra"
	leer cadena2
	cadena3 = Concatenar(cadena1,cadena2);
	Escribir cadena3
FinFuncion
Funcion ejercicio17
	//Ejercicio N°17 //Mayor de tres numeros
	//Definimos n1, n2, n3 como variables enteras
	//Pedimos al usuario que ingrese el primer numero
	//Leemos n1
	//Pedimos al usuario que ingrese el segundo numero
	//Leemos n2
	//Pedimos al usuario que ingrese el tercer numero
	//Leemos n3
	//Usamos el comando Si-Entonces para realizar la operacion
	Escribir "Mayor de tres numeros"
	Definir n1, n2, n3 Como Entero
	Escribir "Ingrese el primer numero"
	leer n1
	Escribir "Ingrese el segundo numero"
	leer n2
	Escribir "Ingrese el tercer numero"
	Leer n3
	Si (n1 > n2 Y n1 > n3) Entonces
		Escribir "El numero mayor es:", n1
	SiNo
		Si (n2 > n1 Y n2 > n3) Entonces
			Escribir "El numero mayor es:", n2
		SiNO 
			Si (n3 > n1 Y n3 > n2) Entonces
				Escribir "El numero mayor es:", n3
			SiNo 
				Escribir "Los numeros son inguales"
			FinSi
		FinSi
	Fin Si
		
FinFuncion

Funcion ejercicio18
	//Ejercicio N°18 "Edad minima para votar"
	//Definimos a,b,w como variables enteras
	//Pedimos al usuario que ingrese el año actual
	//Leemos a
	//Pedimos al usuario que ingrese el año de su nacimiento
	//Leemos b
	//Restamos su edad con el año de su nacimiento y el resultado lo guardamos en la variable w
	//Usamos e<l comando Si-Entonces para realizar el ejercicio
	Escribir "Edad minima para votar"
	Definir a, b, w Como Entero
	Escribir "Ingrese el año actual"
	leer a
	Escribir "Ingrese el año de su nacimiento"
	leer b
	w =  a - b 
	Si w >= 18 Entonces
		Escribir "Tienes edad suficiente para poder votar"
	SiNo
		Escribir "No tienes edad para poder votar"
	Fin Si
FinFuncion

Funcion ejercicio19
	//Ejercicio N°19 "Calculadora de BMI"
	//Definimos peso,altura,res como variables reales
	//Pedimos al usuario que ingrese su peso
	//Leemos peso
	//Pedimos al usuario que ingrese su altura
	//Leemos altura
	//res = peso / altura ^2 
	//Usamos el comando Si-Entonces para ejecutar el ejercicio
	Escribir "Claculadora de BMI"
	Definir peso, altura, res como real
	Escribir "Ingrese su peso (KG)"
	leer peso
	Escribir "Ingrese su altura"
	leer altura
	res = peso / altura^2 
	Escribir "Tu masa corporal es:", res
	Si res = 18.5 Entonces
		Escribir "Su masa corporal es saludable"
	SiNo
		Escribir "Su masa corporal esta fuera del rango saludable"
	Fin Si
FinFuncion

Funcion ejercicio20
	//Ejercicio N°20 "Determinar si un numero es positivo, negativo o cero
	//Definimos num1 como variable entera
	//Pedimos al usuario que ingrese un numero
	//leemos num1
	//Usamos el comando Si-Entonces para realizar el ejercicio
	Escribir "Deerminar si un numero es positivo, negativo o cero"
	Definir num1 como entero
	Escribir "Ingrese un numero"
	leer num1
	Si num1 > 0 Entonces
		Escribir "El numero es positivo"
	SiNo
		Si num1 < 0 Entonces
			Escribir "El numero es negativo"
		SiNo
			Si num1 = 0 Entonces
				Escribir "El numero es 0"
			FinSi
		FinSi
	Fin Si
FinFuncion

Funcion ejercicio21
	//Ejercicio N°21 "Año bisiesto"
	//Definimos la variable año como entero
	//Pedimos al usuario que ingrese un año
	//Leemos año
	//Usar el comando Si-Entonces
	Escribir "Determinar si un año es bisiesto"
	Definir año Como Entero
	Escribir "Ingrese un año"
	leer año
	Si año mod 4 = 0 Y (año mod 100 <> 0) o(año mod 400 = 0) Entonces
		Escribir "El año", año, "es bisiesto"
	SiNo
		Escribir "El año" , año , "no es bisiesto" 
	Fin Si
FinFuncion

Funcion ejercicio22
	//Ejercicio22 "Signo zodiacal"
	//Definimos mes, dia como variables enteras
	//Definimos signo como variable de caracter
	//Pedimos que ingrese su año de nacimiento
	//Leemos año
	//Pedimos que ingrese su dia de nacimiento
	//Leemos dia
	//Usamos el comando Segun para resolver el ejercicio
	Escribir "Signo Zodiacal"
	Definir mes, dia Como Entero
	Definir signo Como Caracter 
	Escribir "Ingrese el dia de su nacimiento"
	leer dia
	Repetir
		
	Hasta Que dia <= 31 Y dia >= 1
    Repetir
		Escribir "Ingrese el mes de su nacimiento"
		Escribir "1) enero"
		Escribir "2) febrero"
		Escribir "3) marzo"
		Escribir "4) abril"
		Escribir "5) mayo"
		Escribir "6) junio"
		Escribir "7) julio"
		Escribir "8) agosto"
		Escribir "9) septiembre"
		Escribir "10) octubre"
		Escribir "11) noviembre"
		Escribir "12) diciembre"
		leer mes
	Hasta que mes >= 1 Y mes <= 12
	
	Segun mes Hacer
		1:
			Si (dia >= 21) 
				signo = "acuario"
			SiNo 
				signo = "capricornio"
		     
			FinSi
		2:
			Si (dia >= 19)
				signo = "psicis"
			SiNo 
				signo = "acuario"
			FinSi
			3:
				Si (dia >= 20)
					signo = "aries"
				SiNo
				signo ="psicis"
			FinSi
		4:
			Si (dia >= 20)
			    signo = "tauro"
			SiNo
				signo = "aries"
			FinSi
		5:
			Si (dia >= 21)
				signo = "geminis"
			SiNo
				signo = "tauro"
			FinSi
		6:
			Si (dia >= 20)
				signo = "cancer"
			SiNo
				signo = "geminis"
			FinSi
		7:
			Si (dia >= 22)
				signo = "leo"
			SiNo
				signo = "cancer"
			FinSi
		8:
			Si (dia >= 21)
				signo = "virgo"
			SiNo
				signo = "leo"
			FinSi
		9:
			Si (dia >= 22)
				signo = "libra"
			SiNo
				signo = "virgo"
			FinSi
		10:
			Si (dia >= 22)
				signo = "escorpion"
			SiNo
				signo = "libra"
			FinSi
		11:
			Si (dia >= 21)
				signo = "sagitario"
			SiNo
				signo = "escorpion"
			FinSi
		12:
			Si (dia >= 21)
				signo = "capricornio"
			SiNo
				signo = "sagitario"
			FinSi
	FinSegun
	Escribir "Su signo zodiacal es:" signo
	
FinFuncion

Funcion ejercicio23
	//Ejercicio N°23 "Dia del mes con respecto a la quincena"
	//Definimos num1 como variables enteras
	//Definimos r como variable de cararcter
	//Pedimos al usuiario que ingrese un numero del mes
	//leemos num1
	//Usamos el comandndo Segun para realizar el ejercicio
	Escribir "Dia del mes con respecto a la quincena"
	Escribir "Ingrese un numero"
	Definir num1 Como Entero
	Definir r como caracter
	Escribir "Ingrese un dia del mes"
	leer num1
	Repetir
		
	Hasta Que num1 <= 31 Y num1 >= 1
	Si (num1 <= 15) Entonces
		Escribir "Este dia pertenece a la primera quincena"
	SiNo
		Si (num1 >= 16) Entonces
			Escribir "Este dia pertenece a la segunda quincena"
		FinSi
	Fin Si
	
FinFuncion

Funcion ejercicio24
	//Ejercicio N°24 "Dia de la semana"
	//Definimos dia como una variable entera
	//Pedimos al usuario que ingrese un numero
	//Leemos dia
	//Usamos el comando Segun para realizar el ejercicio
	Escribir "Dia de la semana"
	Definir dia Como Entero
	Definir res como caracter
	leer dia 
	Repetir
		
	Hasta Que (dia <= 7 ) Y (dia >= 1)
	Segun dia Hacer
		1:
		     Si (dia = 1)
			  res = "Este dia es Lunes"
			FinSi
		2:
		    Si (dia = 2)
			  res = "Este dia es Martes"
		     FinSi
		3:
		    Si (dia = 3)
				res = "Este dia es Miércoles"
			FinSi
		4:
		    Si (dia = 4)
			    res = "Este dia es Jueves"
			FinSi
		5:
		    Si (dia = 5)
			  res = "Este dia es Viernes"
			FinSi
		6:
		    Si (dia = 6)
			   res = "Este dia es Sábado"
		   FinSi
		7:
		Si (dia = 7)
			   res = "Este dia es Domingo"
		FinSi
		De Otro Modo:
			Escribir "Los datos ingresados son incorrectos"
	Fin Segun
	Escribir res
FinFuncion

Funcion ejercicio25
	//Ejercicio N°25 "Comparar palabras"
	//Definimos palabra1, palabra2 como variables de caracter
	//Pedimos al usuario que ingrese una palabra
	//Leer palabra1
	//Pedimos al usuario qe ingrese la segunda palabra
	//Leer palabra2
	//Usamos el comando Si-Entonces para ejecutar el ejercicio
	Escribir "Comparar palabras"
	Definir palabra1, palabra2 Como Caracter
	Escribir "Ingrese una palabra"
	leer palabra1
	Escribir "Ingrese la segunda palabra"
	leer palabra2
	Si palabra1 = palabra2 Entonces
		Escribir "Las palabras son iguales"
	SiNo
		Escribir "Las palabras son diferentes"
	Fin Si
FinFuncion

Funcion ejercicio26
	//Ejercicio N°26 "Calculadora de precio con descuento"
	//Definimos precio, porcentaje, descuento como variables enteras
	//Pedimos al usuario que ingrese el precio
	//Leemos precio
	//Pedimos al usuario que ingrese le valor del porcentaje
	//Leemos descuento
	//Escribimos la variable descuento ya que almacena el total del descuento
	Escribir "Calculadora de precio con descuento"
	Definir precio, porcentaje, total, total2 Como real
	Escribir "Ingrese el precio"
	leer precio
	Escribir "Ingrese el porcentaje de su descuento %"
	leer porcentaje 
	total = precio * porcentaje / 100 
	total2 = precio - total
	Escribir "Su precio final es de:", total2 "$"
	
FinFuncion

Funcion ejercicio27
	//Ejercicio N°27 "Calculadora de factura con impuestos"
	//Definimos total,impuesto,total2 como variables reales
	//Pedimos al usuario que ingrese el total de la compra
	//Leer total
	//Pedimos al usuario que ingrese el valor de los impuestos aplicado
	//Leer imppuesto
	//La variable total2 contendra el valor de la suma entre total e impuesto
	//Escribimos total2
	Escribir "Calculadora de factura com impuestos"
	Definir total, impuesto, total2 Como Real
	Escribir "Ingrese el valor de su compra $"
	leer total
	Escribir "Ingrese el valor de los impuestos a pagar"
	leer impuesto
	total2 = total * impuesto / 100
	total3 = total + total2
	Escribir "El total a pagar por su compra es de:" ,total3,  "$"
	
FinFuncion

Funcion ejercicio28
	//Ejercicio N°28 "Calculadora de sueldo con aumento"
	//Definimos salario, aumento, calculo, suma, nuevosalario como variables reales
	//Pedimos al usuario que ingrese el valor de su salario actual
	//Leer salario
	//Pedimos al usuario que ingrese el porcentaje de aumento de su sueldo
	//Leer aumento
	//calculo = salario * aumento / 100
	//suma = salario + calculo
	//nuevosalario = suma
	Escribir "Calculadora de sueldo con aumento"
	Definir salario, aumento, calculo, suma, nuevosalario Como real
	Escribir "Ingrese el valor de su salario actual $"
	leer salario
	Escribir "Ingrese el porcentaje de aumento de su sueldo %"
	leer aumento
	calculo = salario * aumento / 100
	suma = salario + calculo 
	nuevosalario = suma
	Escribir "Su nuevo salario sera de:" , nuevosalario, "$"
	
FinFuncion

Funcion ejercicio29
	//Ejercicio N°29 "Calculadora de compra con multiples articulos"
	//Definimos precio, cantidad, total como variables reales
	//Pedimos al usuario que ingrese el precio de su compra
	//leer precio
	//Pedimos al usuario que ingrese la cantidad de productos comprados
	//leer cantidad
	//Usamos el comando Si-Entonces para realizar el calculo
	Escribir "Calculadora de compra con multiples articulos"
	Definir precio, cantidad, total, total2, total3 Como Real
	Escribir "Ingrese el precio de su compra $"
	leer precio
	Escribir "Ingrese la cantidad de productos que compro"
	leer cantidad
	total = precio 
	total2 = total
	Si (cantidad >= 12) Entonces
	    total3 = total * 10 / 100
		total4 = total2 - total3
		Escribir "Usted tiene un descuento del 10% por comprar mas de 12 productos y su precio final es de:" ,total4, "$"
	SiNo
		Escribir "El precio final es:" , total, "$"
	Fin Si
	
FinFuncion

Funcion ejercicio(30,32)
	//Ejercicio N°30 "Calculadora de impuestos sobre salario"
	//Definimos las variables salario, res como real
	//Pedimos al usuario que ingrese su salario
	//Leemos salario
	//Usamos el comando Si-Entonces para realizar la operacion
	Escribir "Calculadora de impuestos sobre salario"
	Definir salario, res, total Como real
	Escribir "Ingrese su salario $"
	leer salario
    Si (salario <= 10000) Entonces
		res = salario * 5 / 100
		total = res
	SiNo
		Si (salario > 10001) Y (salario <= 20000) Entonces
			res = salario * 10 / 100
			total = res
		SiNo
			Si (salario > 20000) Entonces
				res = salario * 15 / 100
				total = res
			FinSi
		FinSi
	Fin Si
	
	Escribir "Su impuesto sobre la renta es de:" , total "$"
FinFuncion

