Funcion ejercicio1
	//Ejercicio N°1 "Encontrar el valor de x"
	//Definimos las variables a,b,x como enteras
	//Las variables a=3 y b=7 x=2*a+b-a mod 3
	//Orden de operaciones: (),^,* / mod, + -
	//x = 2 * 3 + 7 - 3 mod 3
	//x = 6 + 7 - 0
	//x = 13
	Definir a,b como entero
	a=3
	b=7
	x=2 * a + b- 3 mod 3
	Escribir "El valor de x es:", x
FinFuncion

  Funcion ejercicio2
//Ejercicio N°2 "Encontrar el valor de z"
//Definimos las variables a,b,z como enteras
//Las variables a=10, b=4 y z=a*b+3 mod a+b
//Orden de operaciones: (),^,* / mod, + -
//z=10*4+3 mod 10+4
//z=40+3+4
//z=47
Definir a,b Como Entero 
a = 10
b = 4
z = a*b+3 mod a+b
Escribir "El valor de z es:" z
FinFuncion

Funcion ejercicio3
//Ejercicio N°3 "Encontrar el valor de w"
//Definimos las variables a,b,w como enteras
//Las variables a=6, b=2 y w=a-b+2*a mod b
//Orden de operaciones: (),^,* / mod, + -
//w=6-2+2*6 mod 2
//w=6-2+12 mod 2
//w=6-2+0
//w = 4
Definir a,b Como Entero
a = 6
b = 2
w = a - b + 2 * a mod b
Escribir "El valor de w es:", w
FinFuncion

Funcion ejercicio4
//Ejercicio N°4 "Encontrar el valor de v"
//Definimos las variables a,b,v como enteras
//Las variables a=8, b=5 y v=2*b+a div 2+4*b mod a
//Orden de operaciones: (),^,* / trunc-mod, + -
//v=2*5+8 trunc 2+4*5 mod 8
//v=10+4+20 mod 8
//v=16
Definir a,b como entero
a = 8
b = 5
v = 2 * b + a / (2 + 4) * (b mod a)
Escribir "El valor de v es:", v
FinFuncion

Funcion ejercicio5
	//Ejercicio N°5 "Encontrar el valor de u"
	//Definimos las variables a,b,u como enteras
	//Las variables a=12, b=9 y u=b-a+3*a mod b
	//Orden de operaciones: (),^,* /, + -
	//u=9-12+3*12 mod 9
	//u=9-12+3-3
	//u=9-12+9
	//u=-3
	Definir a,b como entero
	a = 12
	b = 9
	u = b - a + 3 * a mod b
	Escribir "El valor de u es:", u
FinFuncion

Funcion ejercicio6
	//Ejercicio N°6 "Determinar la veracidad de la siguiente expresion"
	//Definimos las variables conjunto1,conjunto2, valor1, valor2 como entero
	//conjunto1=(5+3*2)+9 > conjunto2=3*5*14 %3
	//Resolver las operaciones y usar el comando Si-Entonces
	//De forma que, Si conjunto1 > conjunto2 Entoces Escribir Verdadero"; SiNO Escribir"Falso"  FinSI
	Definir conjunto1, conjunto2 Como entero
	conjunto1= (5 + 3 * 2) + 9
	conjunto2 = 3 * 5 * 14 %3
	valor1 = conjunto1
	valor2 = conjunto2
	Si valor1 > valor2 Entonces
		Escribir "Es verdadero"
	SiNo
		Escribir"Es falso"
	FinSi
	
FinFuncion

Funcion ejercicio7
	//Ejercicio N°7 "Calcular la operacion"
	//Definimos operacion, resultado como variables reales
	//Donde la variable operacion=2*(4-10+8)/2*36*(1/2)
	//La variable resultado tomara el valor de la variable operacion
	//Escribir "El resultado de la operacion es:" resultado
	Definir operacion, resultado como real
	operacion = 2 * (4 - 10 + 8) / 2 * 36 * (1/2)
	resultado = operacion
	Escribir "El resultado de la operacion es:" resultado
FinFuncion

Funcion ejercicio8
	//Ejercicio N°8 "Calcular la operacion"
	//Definimos operacion, resultado como variables reales
	//La variable operacion= 260/12+54%3-85%3
	//La variable resultado tomara el valor de la variable operacion
	//Escribimos "El resultado de la operacion es:" resultado
	Definir operacion, resultado como real
	operacion = 260 / 12 + 54 % 3 - 85 % 3
	resultado = operacion
	Escribir "El resulatdo de la operacion es:" resultado
	
FinFuncion

Funcion ejercicio9
	//Ejercicio N°9 "Operacion logica"
	//Definimos num1, num2 como variables logicas
	//Definimos condicion como logico
	//Variable num1=(48<2*3)
	//Variable num2=(2*7<12)
	//Variable condicion= num1 o num2;
	//Escribimos la variable condicion
	Definir  num1, num2 Como Logico
	Definir condicion Como Logico
	num1 = (48 < 2 * 3)
	num2 = (2 * 7 < 12)
	condicion = num1 o num2; 
	Escribir condicion
FinFuncion

Funcion ejercicio10
	//Ejercicio N°10 "Operaciones logicas"
	//Definimos num1, num2, num3 como variables logicas
	//Definimos condicion1, condicion2 como logicas 
	//variable num1=(8>2)
	//Variable num2=(932<23)
	//Variable num3=4==2
	//Variable condicion1= num1 o num2;
	//Variable condicion2=num1 o num2 Y num3;
	//Escribimos la variable condicion1
	//Escribimos la variable condicion2
	Definir num1, num2, num3 Como Logico
	Definir condicion1, condicion2 como logico
	num1 = (8>2)
	num2 = (932<23)
	num3 = 4 == 2
	condicion1 = num1 o num2;
	condicion2 = num1 Y num2 Y num3;
	Escribir condicion1
	Escribir condicion2
FinFuncion

